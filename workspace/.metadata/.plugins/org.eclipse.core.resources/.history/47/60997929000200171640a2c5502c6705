package oopsConcepts;

public class Overloading {

	public static void main(String[] args) 
	{
		A obj = new A ();
		obj.show(3);
				
	}

}
class A
{
	public void show()
	{
		System.out.println("Show with no paramater");
	}
	public void show(int i)
	{
		System.out.println("Show with one paramater" +i );
	}
	
}